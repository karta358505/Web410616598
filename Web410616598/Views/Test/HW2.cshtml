
@{
    ViewBag.Title = "HW2";


}
<p>印出1到10</p>
@for (int i = 1; i <= 10; i++)
{
<p>@i</p>
}

<h2>HW2 @DateTime.Now</h2>

<h4>姓名: @Html.TextBox("name")</h4>
<br>
<h4>帳號: @Html.TextBox("account")</h4>
<br>
<h4>密碼: @Html.TextBox("password")</h4>
<br>
<h4>性別: @Html.RadioButton("gender", "A")男性  @Html.RadioButton("gender", "B")女性</h4>
<br>
<h4>興趣: @Html.CheckBox("swimming", "S")游泳  @Html.CheckBox("reading", "R")閱讀 @Html.CheckBox("movie", "M")看電影</h4>
<br>
<h4>自我介紹: <textarea name="message" rows="4" cols="19"></textarea> 